#ifndef  __FRIEND_TIPS__
#define __FRIEND_TIPS__

private void tellDelFriend(object user, string name)
{
	user->tellMe("你已删除好友%s", name);
}

private void tellUserNotExist(object user)
{
	user->tellMe("该用户不存在");
}

private void tellUserBeAdded(object user, string name)
{
	user->tellMe("%s添加您为好友", name);
}

private void tellUserAdd(object user, string name)
{
	user->tellMe("您添加%s为好友", name);
}

private void tellUserHasFriend(object user)
{
	user->tellMe("您已加过该好友");
}

private void tellUserOffline(object user)
{
	user->tellMe("目标玩家不在线");
}































































#endif
